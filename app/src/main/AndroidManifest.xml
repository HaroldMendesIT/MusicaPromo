<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="irshaad.harold.carlos.com.navmenu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NavMenu"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme.windowNoTitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WebAppActivity"
            android:theme="@style/AppTheme.windowNoTitle" />
        <activity
            android:name=".SettingsActivity"
            android:theme="@style/AppTheme.windowNoTitle" />
        <activity
            android:name=".ArtistWebAppActivity"
            android:theme="@style/AppTheme.windowNoTitle" />
        <activity
            android:name=".AlbumWebAppActivity"
            android:theme="@style/AppTheme.windowNoTitle" />
        <activity
            android:name=".GenreWebAppActivity"
            android:theme="@style/AppTheme.windowNoTitle" />
        <activity
            android:name=".SignupWebAppActivity"
            android:theme="@style/AppTheme.windowNoTitle" />
        <activity
            android:name=".LoginWebAppActivity"
            android:theme="@style/AppTheme.windowNoTitle" />
        <activity
            android:name=".SobreNosActivity"
            android:theme="@style/AppTheme.windowNoTitle" />
    </application>

</manifest>